<%= form_tag({action: :import}, multipart: true) do %>
   <div class="field">
     <%= label_tag :contacts_file %>
     <%= file_field_tag :contacts_file %>
   </div>

   <div class="field">
     <%= label_tag :name %>
     <%= text_field_tag "columns[name]" %>
   </div>
   <div class="field">
     <%= label_tag :email %>
     <%= text_field_tag "columns[email]" %>
   </div>
   <div class="field">
     <%= label_tag :birth_date %>
     <%= text_field_tag "columns[birth_date]" %>
   </div>
   <div class="field">
     <%= label_tag :phone %>
     <%= text_field_tag "columns[phone]" %>
   </div>
   <div class="field">
     <%= label_tag :address %>
     <%= text_field_tag "columns[address]" %>
   </div>
   <div class="field">
     <%= label_tag :credit_card %>
     <%= text_field_tag "columns[credit_card]" %>
   </div>
   <div class="actions">
      <%= submit_tag 'Submit' %>
   </div>
<% end %>